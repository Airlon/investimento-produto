pipeline {
    agent none
    stages {
        stage('Build') {
            agent {
                docker { image 'maven' }
            }
            steps {
                sh 'echo "Deploy arquivo!"'
            }
        }
    }

    post {
        success {
            echo 'Pipeline executado com sucesso!'
        }
        failure {
            echo 'Ocorreu uma falha no pipeline'
        }
    }
}
